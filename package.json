{
  "name": "chat-history-manager",
  "version": "1.0.0",
  "description": "Cursorエディタのチャット履歴を管理・検索・分析するための包括的なツール",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsc && node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "server": "tsc && node dist/server/app.js",
    "web": "cd web && vite",
    "web:build": "cd web && vite build",
    "dev:full": "concurrently \"npm run server\" \"npm run web\"",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix"
  },
  "keywords": [
    "cursor",
    "chat",
    "history",
    "conversation",
    "storage",
    "search",
