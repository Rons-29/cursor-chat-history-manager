{
  "name": "chat-history-manager",
  "version": "1.0.0",
  "description": "Cursorエディタのチャット履歴を管理・検索・分析するための包括的なツール",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "tsc && node dist/index.js",
    "build": "tsc",
    "server": "tsc && node dist/server/app.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "web:dev": "vite",
    "web:build": "vite build",
    "dev:full": "concurrently \"npm run dev\" \"npm run web:dev\"",
    "integration": "tsc && node dist/cli-integration.js",
    "integration:scan": "npm run integration scan",
    "integration:watch": "npm run integration watch",
    "integration:status": "npm run integration status",
    "integration:config": "npm run integration config",
    "real-server": "tsc && node dist/server/real-api-server.js",
    "test:real-integration": "tsc && node dist/scripts/test-real-integration.js",
    "dev:real": "concurrently \"npm run real-server\" \"npm run web:dev\""
  },
  "keywords": [
    "cursor",
    "chat",
    "history",
    "conversation",
    "storage",
    "search",
    "ai",
    "assistant",
    "development",
    "tools"
  ],
  "author": "Rons-29",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rons-29/cursor-chat-history-manager.git"
  },
  "bugs": {
    "url": "https://github.com/Rons-29/cursor-chat-history-manager/issues"
  },
  "homepage": "https://github.com/Rons-29/cursor-chat-history-manager#readme",
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@tailwindcss/postcss": "^4.1.8",
    "@tanstack/react-query": "^5.79.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/compression": "^1.8.0",
    "@types/cors": "^2.8.18",
    "@types/morgan": "^1.9.9",
    "@types/recharts": "^1.8.29",
    "@vitejs/plugin-react": "^4.5.0",
    "autoprefixer": "^10.4.21",
    "better-sqlite3": "^11.10.0",
    "chokidar": "^3.5.3",
    "commander": "^14.0.0",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "events": "^3.3.0",
    "express": "^5.1.0",
    "fs-extra": "^11.2.0",
    "helmet": "^8.1.0",
    "morgan": "^1.10.0",
    "postcss": "^8.5.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.6.1",
    "recharts": "^2.15.3",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "tailwindcss": "^4.1.8",
    "typescript": "^5.3.3",
    "uuid": "^9.0.1",
    "vite": "^6.3.5",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
